<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
assembly="CWM.Core"
namespace="CWM.Core.Domain">
    <class name="Poll" table="Poll">
        <id name="ID" column="ID">
            <generator class="identity"></generator>
        </id>
        <property name="Name" />
        <property name="StartDate" />
        <property name="EndDate" />
        <property name="Question" />
        <property name="IsActive" />
        <bag name="Options" table="PollOption" generic="true" inverse="true" cascade="none">
            <key column="PollID"/>
            <many-to-many column="ID" class="CWM.Core.Domain.PollOption, CWM.Core" />
        </bag>
    </class>
</hibernate-mapping>